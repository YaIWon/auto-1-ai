{
  "name": "autonomous-ai-browser-extension",
  "version": "4.2.1",
  "description": "Autonomous AI System Browser Extension",
  "scripts": {
    "build": "node build.js",
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "package": "npm run prod && npm run zip",
