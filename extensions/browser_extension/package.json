    "deploy": "npm run package && gh-pages -d dist"
  },
  "dependencies": {
    "web3": "^4.0.0",
    "axios": "^1.5.0",
    "ws": "^8.14.0",
    "buffer": "^6.0.0"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "zip-webpack-plugin": "^4.0.0",
    "gh-pages": "^5.0.0"
  }
}
